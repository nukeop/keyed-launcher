{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    "clipboard-manager:default",
    "shell:default",
    "shell:allow-execute",
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "**/*"
        }
      ]
    }
  ]
}