{
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "importOrder": [
        "^@?(.*)$",
        "<BUILTIN_MODULES>",
        "<THIRD_PARTY_MODULES>",
        " ",
        " ",
        "^[./]",
        "^[../]"
    ],
    "importOrderParserPlugins": [
        "typescript",
        "jsx"
    ],
    "plugins": [
        "prettier-plugin-tailwindcss",
        "@ianvs/prettier-plugin-sort-imports"
    ]
}